# Mistral configuration
MISTRAL_API_KEY=your_mistral_key_here

# Cohere configuration
COHERE_API_KEY=your_cohere_key_here

# Groq configuration
GROQ_API_KEY=your_groq_key_here

# Logging configuration
LOGFIRE_ENABLED=true
# Your Logfire token
LOGFIRE_TOKEN=
